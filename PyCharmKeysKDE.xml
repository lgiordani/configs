<keymap version="1" name="KDE Keys Leo" parent="Default for KDE">
  <action id="CloseAllEditors">
    <keyboard-shortcut first-keystroke="shift ctrl alt w" />
  </action>
  <action id="CloseAllEditorsButActive">
    <keyboard-shortcut first-keystroke="shift ctrl w" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="DumpLookupElementWeights" />
  <action id="EditorAddOrRemoveCaret">
    <mouse-shortcut keystroke="meta button1" />
  </action>
  <action id="EditorCloneCaretAbove">
    <keyboard-shortcut first-keystroke="shift meta up" />
  </action>
  <action id="EditorCloneCaretBelow">
    <keyboard-shortcut first-keystroke="shift meta down" />
  </action>
  <action id="EditorSelectWord" />
  <action id="EditorUnSelectWord" />
  <action id="IntroduceVariable">
    <keyboard-shortcut first-keystroke="ctrl alt v" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
</keymap>